{
    "title":"Notes",
    "description": "学习笔记",
    "author":"B.W.H",
    "language": "zh-hans",
    "root":".",
    "plugins": [
        "simple-page-toc",
        "code",
        "copy-code-button",
        "expandable-chapters",
        "expandable-chapters-small",
        "back-to-top-button",
        "-lunr", 
        "-search", 
        "search-pro",
        "edit-link",
        "github",
        "hide-element"
   ],
    "pluginsConfig": {
        "edit-link": {
          "base" : "https://github.com/bwh-yjh/Notes/blob/master",
          "label" : "Edit This Page"
        },
        "github": {
          "url": "https://github.com/bwh-yjh/Notes/"
        },
         "hide-element": {
            "elements": [".gitbook-link"]	
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        }
  }

}
